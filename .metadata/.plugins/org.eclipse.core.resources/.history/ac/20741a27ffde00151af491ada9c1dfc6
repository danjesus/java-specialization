public class Principal {

	public static void main(String[] args) {
		Paciente p1 = new Paciente (170.0, 3.0);
		Paciente p2 = new Paciente (69.0, 1.68);
		Paciente p3 = new Paciente (40.0, 1.10);
		
		System.out.println("IMC foi: " + p1.calcularIMC() + " que corresponde " + p1.diagnostico());
		System.out.println("IMC foi: " + p2.calcularIMC() + " que corresponde " + p2.diagnostico());
		System.out.println("IMC foi: " + p3.calcularIMC() + " que corresponde " + p3.diagnostico());
	}

}
